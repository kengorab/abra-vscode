{
  "name": "abra",
  "displayName": "Abra",
  "description": "Official extension for the Abra programming language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "abra",
        "aliases": [
          "Abra",
          "abra"
        ],
        "extensions": [
          ".abra"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "abra",
        "scopeName": "source.abra",
        "path": "./syntaxes/abra.tmLanguage.json"
      }
    ]
  }
}
